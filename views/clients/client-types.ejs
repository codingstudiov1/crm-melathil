<section class="content-header">
  <div class="container-fluid">
    <div class="row mb-2">
      <div class="col-sm-6">
        <h1>
          <%= title %>
        </h1>
      </div>
      <div class="col-sm-6">
        <ol class="breadcrumb float-sm-right">
          <li class="breadcrumb-item"><a href="/admin">Home</a></li>
          <li class="breadcrumb-item active">
            <%= title %>
          </li>
        </ol>
      </div>
    </div>
  </div>
</section>
<section>
  <div class="content">
    <div class="container">
      <a class="btn btn-primary mb-3" href="/admin/create-client-type">Create new</a>
      <div class="row">
        <div class="col-md-8">
          <div class="table-responsive">
            <div class="card">
              <div class="card-body">
                <table class="table">
                  <thead>
                    <tr>
                      <th>Serial</th>
                      <th>Name</th>
                      <th colspan="2">Operaton</th>
                    </tr>
                  </thead>
                  <tbody>
                    <% clientType.forEach((element,index)=> { %>
                    <tr>
                      <td>
                        <%= index+1 %>
                      </td>
                      <td>
                        <%= element.name %>
                      </td>
                      <td><a class="text-primary" onclick="return confirm('Are you sure to modify')" href="/admin/modify-client-type/<%= element._id %> ">Edit</a>
                      </td>
                      <td><a class="text-danger" onclick="return confirm('Are you sure to delete')" href="/admin/delete-client-type/<%= element._id %> ">Delete</a>
                      </td>

                    </tr>
                    <% }) %>
                  </tbody>
                </table>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>