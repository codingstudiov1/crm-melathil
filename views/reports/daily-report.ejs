<section class="content-header">
  <div class="container-fluid">
    <div class="row mb-2">
      <div class="col-sm-6">
        <h1>
          <%= title %>
        </h1>
      </div>
      <div class="col-sm-6">
        <ol class="breadcrumb float-sm-right">
          <li class="breadcrumb-item"><a href="/dashboard/home">Home</a></li>
          <li class="breadcrumb-item active">
            <%= title %>
          </li>
        </ol>
      </div>
    </div>
  </div>
</section>


<section>
  <div class="row">
    <div class="col-12">
      <div class="card">
        <div class="card-body">
          <div class="table-responsive">
            <table class="table table-bordered" id="example1">
              <thead>
                  <tr>
                      <td colspan="6">Daily Report Date <%= moment(date).format('DD-MM-YYYY') %></td>
                  </tr>
                <tr>
                  <th>Sl.No.</th>
                  <th>EnqId</th>
                  <th>Date</th>
                  <th>Status</th>
                  <th>Temparature</th>
                  <th>Remarks</th>
                </tr>
              </thead>
              <tbody>
                <% report.forEach((element,index) => { %>
                <tr>
                  <td><%= index+1 %></td>
                  <td><a href="/dashboard/enquiries/view/<%= element.enquiryid %>"><%= element.enquiryid %></a></td>
                  <td><%= moment(element.date).format('DD-MM-YYYY') %></td>
                  <td><%= element.status %></td>
                  <td><%= element.temparature %></td>
                  <td><%= element.remarks %></td>
                </tr>
                <% }) %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>