<section class="content-header">
  <div class="container-fluid">
    <div class="row mb-2">
      <div class="col-sm-6">
        <h1>
          <%= title %>
        </h1>
      </div>
      <div class="col-sm-6">
        <ol class="breadcrumb float-sm-right">
          <li class="breadcrumb-item"><a href="/dashboard/home">Home</a></li>
          <li class="breadcrumb-item active">
            <%= title %>
          </li>
        </ol>
      </div>
    </div>
  </div>
</section>
<section>
  <div class="content">
    <div class="container">
      <a class="btn btn-primary mb-3" href="/dashboard/enquiries/create">Create new</a>
      <div class="row">
        <div class="col-12">
          <div class="card">
            <div class="card-body">
              <div class="table-responsive">
                <table class="table" id="example1">
                  <thead>
                    <tr>
                      <th>Serial</th>
                      <th>Status</th>
                      <th>Date</th>
                      <th>Name</th>
                      <th colspan="3">Operaton</th>
                    </tr>
                  </thead>
                  <tbody>
                    <% enquiries.forEach((element,index)=> { %>
                    <tr>
                      <td>
                        <%= index+1 %>
                      </td>
                      <td>
                        <% if (element.status===0) { %>
                        <div class="badge bg-warning">Working</div>
                        <% } else if (element.status===1) { %>
                        <div class="badge bg-success">Closed</div>
                        <% } else if (element.status===2) { %>
                        <div class="badge bg-danger">Failed</div>
                        <% } %>
                      </td>
                      <td>
                        <%= moment(element.date).format("DD-MM-YYYY") %>
                      </td>
                      <td>
                        <%= element.title %>
                      </td>
                      <td>
                        <a class="text-primary" href="/dashboard/enquiries/view/<%= element.id %> ">View Details</a>
                      </td>
                      <td><a class="text-primary" onclick="return confirm('Are you sure to modify')" href="/dashboard/clients/client-types/modify/<%= element.id %> ">Edit</a>
                      </td>
                    </tr>
                    <% }) %>
                  </tbody>
                </table>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>