<section class="content-header">
  <div class="container-fluid">
    <div class="row mb-2">
      <div class="col-sm-6">
        <h1>Active employees</h1>
      </div>
      <div class="col-sm-6">
        <ol class="breadcrumb float-sm-right">
          <li class="breadcrumb-item"><a href="/">Home</a></li>
          <li class="breadcrumb-item active">Active employees</li>
        </ol>
      </div>
    </div>
  </div>
</section>
<section class="content">
  <div class="container-fluid">
    <div class="card">
      <div class="card-body">

        <table class="table">
          <thead>
            <tr>
              <th>Sl No</th>
              <th>User Id</th>
              <th>Name</th>
              <th>Mobile</th>
              <th>Email</th>
              <th>Gender</th>
            </tr>
          </thead>
          <tbody>
            <% employees.forEach((element,index)=> { %>
            <tr>
              <td>
                <%= index+1 %>
              </td>
              <td>
                <%= element.id %>
              </td>
              <td>
                <a href="/<%= route %>/employees/<%= element.id %>"> <%= element.firstName %>
                  <%= element.lastName %></a>
              </td>
              <td>
                <%= element.phone %>
              </td>
              <td>
                <%= element.email %>
              </td>
              <td>
                <%= element.gender %>
              </td>
            </tr>
            <% }) %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</section>